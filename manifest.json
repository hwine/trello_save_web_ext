{

  "description": "Adds a button to save Trello Board as JSON",
  "manifest_version": 2,
  "name": "Trello Save",
  "version": "0.1",
  "homepage_url": "https://github.com/hwine/trello_save_web_ext",

  "content_scripts": [
    {
      "matches": ["*://trello.com/b/*"],
      "js": ["export_json.js"]
    }
  ]

}
